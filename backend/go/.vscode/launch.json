
{
    "version": "0.2.0",
    "configurations": [
          {
                "name": "Delve",
                "type": "go",
                "request": "launch",
                "mode": "auto",
                "program": "${workspaceFolder}",
                "showLog": true,
                "args": [
                      "-c",
                      "data.json"
                ]
          },
          {
                "name": "Delve in Docker",
                "type": "go",
                "request": "attach",
                "mode": "remote",
                "remotePath": "/src",
                "port": 40000,
                "host": "127.0.0.1",
                "showLog": true
          }
    ]
}
